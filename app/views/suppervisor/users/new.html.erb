<% provide(:title, 'Create member') %>
<h1>Create new member</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for [:suppervisor, @user] do |f| %>
      <%= render 'fields', f: f %>
      <%= f.submit "Create member", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>